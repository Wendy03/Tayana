                            <div class="pcoded-content">
                                <!-- Page-header start -->
                                <!-- Page-header end -->
                                <div class="pcoded-inner-content">
                                    <!-- Main-body start -->
                                    <div class="main-body">
                                        <div class="page-wrapper">
                                            <!-- Page-body start -->
                                            <asp:ContentPlaceHolder ID="AdminContentPlaceHolder" runat="server">
                                            </asp:ContentPlaceHolder>
                                            <!-- Page-body end -->
                                        </div>
                                        <div id="styleSelector"></div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
