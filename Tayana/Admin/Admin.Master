    <script type="text/javascript" src="../assets/admin/js/script.js "></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/pdf.js/2.9.359/pdf.min.js"></script>
    <script src="/ckfinder/ckfinder.js"></script>
